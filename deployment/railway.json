{
  "services": [
    {
      "name": "lms-backend",
      "startCommand": "npm start",
      "buildCommand": "npm install",
      "env": {
        "PORT": "5000",
        "MONGODB_URI": "<your_mongo_connection_uri>",
        "JWT_SECRET": "<your_jwt_secret>"
      }
    }
  ],
  "databases": [
    {
      "name": "lms_mongo",
      "type": "mongodb",
      "plan": "starter"
    }
  ]
}
